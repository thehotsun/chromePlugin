{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/a.svg"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://juejin.cn/*"
      ],
      "js": [
        "myscript.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "contextMenus",
    "tabs"
  ],
  "manifest_version": 2
}